{
    "name": "bigfishtv/magicmenu",
    "description": "A CakePHP Plugin to generate HTML menus",
    "type": "cakephp-plugin",
    "authors": [
        {
            "name": "Adam Royle",
            "email": "adam@bigfish.tv"
        }
    ],
    "require": {
		"cakephp/cakephp": "~3.0"
	},
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {"MagicMenu\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "MagicMenu\\Test\\": "tests"
        }
    }
}
